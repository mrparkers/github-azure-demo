{
  "name": "dotnet-react-demo-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^10.0.4",
    "chance": "^1.1.5",
    "cross-env": "^5.1.4",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@babel/core": "^7.9.6",
