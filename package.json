    "seamless-immutable": "^7.1.3",
    "style-loader": "0.19.0",
    "superagent": "^3.8.2",
    "superagent-promise": "^1.1.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "coverage": "cross-env NODE_ENV=test jest --coverage",
    "dev": "cross-env PORT=4100 node scripts/start.js",
    "lint": "prettier --check src/client/**/*.js",
    "lintfix": "prettier --write src/client/**/*.js",
    "test": "cross-env NODE_ENV=test jest"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/client/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/client/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/client/**/?(*.)(spec|test).{js,jsx,mjs}"
    ]
  },
  "prettier": {
    "tabWidth": 4
  }
}
